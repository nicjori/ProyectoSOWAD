<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ws_seguridadService WSDL service -->

  <service name="ws_seguridad" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://seguridad.ws.com"/>
      <parameter name="wsdlServiceElement" value="ws_seguridadService"/>
      <parameter name="schemaQualified" value="http://seguridad.ws.com,http://Comun.ent.com,http://Seguridad.ent.com"/>
      <parameter name="wsdlServicePort" value="ws_seguridad"/>
      <parameter name="className" value="com.ws.seguridad.Ws_seguridadSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ws_seguridad"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="verificarAccesoEntero" qname="operNS:verificarAccesoEntero" xmlns:operNS="http://seguridad.ws.com" returnQName="retNS:verificarAccesoEnteroReturn" xmlns:retNS="http://seguridad.ws.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:_Usuario" xmlns:pns="http://seguridad.ws.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:_Passsword" xmlns:pns="http://seguridad.ws.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="verificarAcceso" qname="operNS:verificarAcceso" xmlns:operNS="http://seguridad.ws.com" returnQName="retNS:verificarAccesoReturn" xmlns:retNS="http://seguridad.ws.com" returnType="rtns:SEG_UsuarioEnt" xmlns:rtns="http://Seguridad.ent.com" soapAction="" >
        <parameter qname="pns:_Usuario" xmlns:pns="http://seguridad.ws.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:_Passsword" xmlns:pns="http://seguridad.ws.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="verificarAcceso verificarAccesoEntero"/>

      <typeMapping
        xmlns:ns="http://Comun.ent.com"
        qname="ns:CMM_PersonaEnt"
        type="java:com.ent.Comun.CMM_PersonaEnt"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://Seguridad.ent.com"
        qname="ns:SEG_UsuarioEnt"
        type="java:com.ent.Seguridad.SEG_UsuarioEnt"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
