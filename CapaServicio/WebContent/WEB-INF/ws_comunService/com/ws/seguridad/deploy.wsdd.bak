<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ws_comunService WSDL service -->

  <service name="ws_comun" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://seguridad.ws.com"/>
      <parameter name="wsdlServiceElement" value="ws_comunService"/>
      <parameter name="schemaQualified" value="http://seguridad.ws.com,http://Comun.ent.com"/>
      <parameter name="wsdlServicePort" value="ws_comun"/>
      <parameter name="className" value="com.ws.seguridad.Ws_comunSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ws_comun"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="listapersona" qname="operNS:listapersona" xmlns:operNS="http://seguridad.ws.com" returnQName="retNS:listapersonaReturn" xmlns:retNS="http://seguridad.ws.com" returnType="rtns:CMM_PersonaEnt" xmlns:rtns="http://Comun.ent.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="listapersona"/>

      <typeMapping
        xmlns:ns="http://Comun.ent.com"
        qname="ns:CMM_PersonaEnt"
        type="java:com.ent.Comun.CMM_PersonaEnt"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
